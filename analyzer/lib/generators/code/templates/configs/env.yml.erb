---
R: <%= Tools::Dimension::R %> # <%= Tools::Dimension::R_DIMENSION %>

temperature:
  gas: <%= gas_temperature %> # <%= Tools::Dimension::DEFAULT_TEMPERATURE %>
  surface: <%= surface_temperature %> # <%= Tools::Dimension::DEFAULT_TEMPERATURE %>

concentrations:
<% gas_species.each do |spec| %>
  <%= concentration_name(spec) %>: <%= concentration_value(spec) %> # <%= Tools::Dimension::DEFAULT_CONCENTRATION %>
<% end %>
