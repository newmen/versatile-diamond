#ifndef <%= define_name %>
#define <%= define_name %>

<% iterator_files.each do |file_name| %>
#include "../../phases/<%= file_name %>.h"
<% end %>
<% header_species_dependencies.each do |specie| %>
#include "../<%= specie.full_file_path %>.h"
<% end %>
<% unless symmetric? %>
#include "../<%= outer_base_file %>.h"
<% end %>

class <%= class_name %> :
    <%= public_inheritance(base_classes) %>
{
<% if render_find_algorithms? %>
<%= render_partial('find_algorithms.h') %>
public:
<% else %>
protected:
<% end %>
    <%= class_name %>(<%= constructor_arguments_str %>) :
        <%= outer_base_call %> {}

public:
#ifdef PRINT
    const char *name() const final;
#endif // PRINT

protected:
    const ushort *indexes() const final { return __indexes; }
    const ushort *roles() const final { return __roles; }

private:
    static const ushort __indexes[<%= atoms_num %>];
    static const ushort __roles[<%= atoms_num %>];
};

#endif // <%= define_name %>
