#ifndef <%= define_name %>
#define <%= define_name %>

<% iterator_files.each do |file_name| %>
#include "../../phases/<%= file_name %>.h"
<% end %>
<% header_species_dependencies.each do |specie| %>
#include "../<%= specie.full_file_path %>.h"
<% end %>
<% unless symmetric? && render_find_algorithms? %>
#include "../<%= outer_base_file %>.h"
<% end %>

class <%= class_name %> :
    <%= public_inheritance(base_classes) %>
{
<% if render_find_algorithms? %>
<%= render_partial('find_algorithms.h') %>
public:
<% else %>
protected:
<% end %>
    <%= class_name %>(<%= constructor_arguments_str %>) :
        <%= outer_base_call %> {}

<% unless symmetric? && render_find_algorithms? %>
public:
#ifdef PRINT
    const char *name() const final;
#endif // PRINT
<% end %>
};

#endif // <%= define_name %>
